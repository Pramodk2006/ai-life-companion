{
  "version": 2,
  "functions": {
    "src/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CLOUD_MODE": "true",
    "DISABLE_CONSOLE": "true"
  },
  "build": {
    "env": {
      "NPM_FLAGS": "--prefer-offline"
    }
  }
}